<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Worldbuilder" x:Class="Worldbuilder.MainWindow"
        Title="Worldbuilder by davidsyntex" Height="468" Width="477">
    <Grid>
        <TextBlock x:Name="TextBlockDate" HorizontalAlignment="Left" Margin="166,17,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBox x:Name="TextboxOutput" Margin="10,50,10,10" TextWrapping="Wrap" Text="TextBox" VerticalScrollBarVisibility="Auto" IsReadOnly="True"/>
        <Button x:Name="ButtonCreateWorld" Content="Create world" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="ButtonCreateWorldClicked" BorderBrush="#FFFF5929" Background="White" FontSize="16"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="384,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="304,10,0,0" VerticalAlignment="Top" Width="75"/>
        
    </Grid>
</Window>
